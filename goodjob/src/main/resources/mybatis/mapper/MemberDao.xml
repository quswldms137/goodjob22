<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.example.goodjob.dao.IMemberDao">
	<select id="findByUsername" resultType="com.example.goodjob.dto.MemberDto">
		SELECT * FROM member WHERE username = #{username};
	</select>
	<update id="update">
		UPDATE member SET(NULL, #{memberDto.username}, #{memberDto.mem_name}, #{memberDto.mem_tel}, #{memberDto.mem_email}, #{memberDto.mem_gender}, #{memberDto.mem_birth}, #{memberDto.mem_addr});
	</update>
</mapper>  